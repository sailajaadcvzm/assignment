#include <stdio.h>

int main() {
    int i;
    char s[10];
    printf("enter string");
    scanf("%s",s);
    for(i=0;s[i]!=0;i++)
    {
        if(s[i]>='A' && s[i]<='Z')
        s[i]=s[i]+1;
        else if(s[i]>='a' && s[i]<='z')
        s[i]=s[i]+1;
        }
    printf("%s",s); 
    return 0;
}
